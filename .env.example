# Sample environment configuration
# Copy this to .env and fill in your values

# xAI Grok API Key (get from x.ai)
XAI_API_KEY=your-xai-api-key-here

# x402 Payment Config
X402_ENABLED=true
X402_NETWORK=base-sepolia
X402_FACILITATOR_URL=https://x402.org/facilitator

# Orchestrator Wallet (Base Sepolia)
ORCHESTRATOR_WALLET=0xYourOrchestratorWalletAddress
ORCHESTRATOR_PRIVATE_KEY=0xYourOrchestratorPrivateKey

# Base Sepolia RPC (optional, defaults to https://sepolia.base.org)
BASE_SEPOLIA_RPC=https://sepolia.base.org

# Query price in USDC
QUERY_PRICE_USDC=0.10

# On-chain distribution (set to true to distribute USDC to agents on-chain)
ONCHAIN_DISTRIBUTION=true
